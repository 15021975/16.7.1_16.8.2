class Cat:
    def __init__(self, name, age,gender):
        self.name = name #кличка животного
        self.age = age  #возраст животного
        self.gender = gender  #пол животного
    def info_Cat(self):
        print(f'кличка животного: {self.name}",\n возраст животного: {self.age},\n пол животного: {self.gender}')
